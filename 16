num1=float(input("num1="))
num2=float(input("num2="))
num3=float(input("num3="))
if num1<=0 or num2<=0 or num3 <=0:
    print("违法输入")
#while(num1+num2>num3,num2+num3>num1,num1+num3>num2):
#    print("yes")
#    break
#else:
#    print("no")
elif num1+num2>num3 and num2+num3>num1 and num1+num3>num2:
    print("yes")
else:
    print("no")
