def func(s):
    list=[]
    for i in s:
        if list[-2]!=i or not list:

#在循环遍历list从末尾开始第二个与倒数第一个进行比较

            list.append(i)
            list.append(i)

#存在相同的即在其1本体后面+1本次判断

        else:
            list[-1]+=1

#让list元素轮到下一个

    return ''.join(map(str,[x for x in list if x !=-1]))

print(func('aabbbcdfg'))
